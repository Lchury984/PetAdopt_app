GEMINI_API_KEY=

# OAuth configuration (if used)
SUPABASE_GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com
SUPABASE_GOOGLE_REDIRECT_URL=io.supabase.flutter://login-callback
